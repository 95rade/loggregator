#!/bin/bash

set -e

$(dirname $0)/go clean deaagent/deaagent
$(dirname $0)/go clean loggregator/loggregator
$(dirname $0)/go clean loggregatorrouter/loggregatorrouter

$(dirname $0)/go build -v deaagent/deaagent
$(dirname $0)/go build -v loggregator/loggregator
$(dirname $0)/go build -v loggregatorrouter/loggregatorrouter
