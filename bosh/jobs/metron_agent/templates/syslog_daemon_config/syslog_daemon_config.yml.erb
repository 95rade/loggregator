---
domain: <%= properties.domain %>

<% if properties.syslog_daemon_config.log_email %>
enabled: <%= properties.syslog_daemon_config.log_email.enabled %>
from_email: <%= properties.syslog_daemon_config.log_email.from_email %>
from_password: <%= properties.syslog_daemon_config.log_email.from_password %>
to_email: <%= properties.syslog_daemon_config.log_email.to_email %>
smtp_server: <%= properties.syslog_daemon_config.log_email.smtp_server %>
smtp_port: <%= properties.syslog_daemon_config.log_email.smtp_port %>
smtp_domain: <%= properties.syslog_daemon_config.log_email.smtp_domain %>
<% end %>
